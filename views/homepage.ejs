<%- include("header") -%>
        <h1>Home</h1>
        <p><%= homeParagraph %> </p>
        
         <% for(let i = 0; i < postTitle.length; i++) { %>
                <div class="">
                <h2><%= postTitle[i] %> </h2>
                <%if (post[i].length > 100) { %>
                    <p><%= post[i].substring(0,100) %> <a href="post/<%= postTitle[i] %>">...Read More</a> </p>
                    <% } else { %>
                        <p><%= post[i] %>  </p>
                    <% } %> 
            </div>
        <% } %>  
        
<%- include("footer") -%>